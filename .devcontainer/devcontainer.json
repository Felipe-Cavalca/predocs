{
  "name": "DevContainer PreDocs",
  "dockerComposeFile": "../Docker/docker-compose.yml",
  "service": "predocs",
  "workspaceFolder": "/var/www/html",
  "forwardPorts": [
    7000, // Local port - PreDocs
    8000, // Frontend
    9000 // Backend
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "felixfbecker.php-debug",
        "mhutchie.git-graph"
      ]
    }
  }
}
